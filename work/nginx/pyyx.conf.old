server {
    listen 8080;
    server_name api.pyyx.com;
    root /opt/pyyx_api/api/api/public;
    try_files $uri $uri/index.html @fpm;
    location @fpm {
        internal;
        include fastcgi_params;
        fastcgi_pass unix:/run/php-fpm.sock;
        fastcgi_param SCRIPT_FILENAME /opt/pyyx_api/api/api/public/index.php;
        }
    location  ~.*\.php?$ {
        include fastcgi_params;
        fastcgi_pass unix:/run/php-fpm.sock;
        fastcgi_param SCRIPT_FILENAME $document_root$fastcgi_script_name;
    }
}
server {
    listen 8080;
    server_name website.pyyx.com;
    root /opt/pyyx_website/website/website/public;
    index index.php;
    if (!-e $request_filename){
        rewrite ^(.*)$ /index.php;
    }
    location  ~ .*\.php$ {
        include fastcgi_params;
        fastcgi_pass unix:/run/php-fpm.sock;
        fastcgi_param SCRIPT_FILENAME $document_root$fastcgi_script_name;
    }
}

