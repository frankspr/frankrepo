server {
    listen 8080;
    server_name url.pyyx.com;
    proxy_set_header Host pyyx.com;
    proxy_set_header X-Real-IP $remote_addr;
    proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;

    location / {
        limit_req zone=rate20 burst=10 nodelay;
        proxy_pass http://127.0.0.1:8080/shorturl/;
    }
}

server {
    listen 8080;
    server_name test.pengyouyinxiang.com test.pyyx.com;
    root /opt/pyyx/test/static;
    try_files $uri $uri/index.html @fpm;

    location @fpm {
        internal;
        include fastcgi_params;
        fastcgi_pass unix:/run/php-fpm.sock;
        fastcgi_param SCRIPT_FILENAME /opt/pyyx/test/web.php;
    }
}


server {
    listen 8080;
    server_name api.pyyx.com;
    root /opt/pyyx_api/api/api/public;
    try_files $uri $uri/index.html @fpm;
    location @fpm {
        internal;
        include fastcgi_params;
        fastcgi_pass unix:/run/php-fpm.sock;
        fastcgi_param SCRIPT_FILENAME /opt/pyyx_api/api/api/public/index.php;
        }
    location  ~.*\.php?$ {
        include fastcgi_params;
        fastcgi_pass unix:/run/php-fpm.sock;
        fastcgi_param SCRIPT_FILENAME $document_root$fastcgi_script_name;
    }
}
server {
    listen 8080;
    server_name website.pyyx.com;
    root /opt/pyyx_website/website/website/public;
    index index.php;
    if (!-e $request_filename){
        rewrite ^(.*)$ /index.php;
    }
    location  ~ .*\.php$ {
        include fastcgi_params;
        fastcgi_pass unix:/run/php-fpm.sock;
        fastcgi_param SCRIPT_FILENAME $document_root$fastcgi_script_name;
    }
}

