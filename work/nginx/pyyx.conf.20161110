    root /opt/pyyx/prod/static;
    try_files $uri $uri/index.html @fpm;

    location @fpm {
        internal;
        include fastcgi_params;

        limit_req zone=rate20 burst=10 nodelay;
        fastcgi_connect_timeout 300;
        fastcgi_send_timeout 300;
        fastcgi_read_timeout 300;
